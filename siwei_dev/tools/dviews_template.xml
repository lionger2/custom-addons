	        <field name="res_model">anodoo.login.record</field>
	        <field name="view_mode">tree,form</field>
	        <field name="context">{}</field>
	    </record>
	    
	    <menuitem parent="cust_menu_config_segment" id="cust_menu_config_segment_anodoo_login_record" name="用户登陆记录" action="action_anodoo_login_record_list"/>
	    
	    
	    
	    
	    