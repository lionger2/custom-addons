		<!-- 用户登陆记录 model_id: anodoo.login.record-->
	    <record id="view_anodoo_login_record_form" model="ir.ui.view">
	        <field name="name">anodoo.login.record.view.form</field>
	        <field name="model">anodoo.login.record</field>
	        <field name="priority">100</field>
	        <field name="arch" type="xml">
	            <form string="anodoo_login_record">
	                <sheet>
		                <group name="group_top">
			                 <group name="group_left">
			                     <field name="code" />
			                     <field name="name" />
			                     <field name="sequence" />
			                 </group>
			                 <group name="group_right">
			                     <field name="type" />
			                     <field name="online" />
			                     <field name="description" />
			                 </group>
			             </group>
			             <notebook colspan="4">
							<page name="location" string="位置信息" autofocus="autofocus">
							    <field name="location_ids" context="{'territory_id': id}">
			                     	<tree editable='bottom'>
						                <field name="country_id" />
						                <field name="state_id" />
						                <field name="city_id" />            
						            </tree>
			                     </field>
							</page>
						</notebook>
	                </sheet>
	            </form>            
	        </field>
	    </record>
	    <record id="view_anodoo_login_record_list" model="ir.ui.view">
	        <field name="name">anodoo.login.record.view.list</field>
	        <field name="model">anodoo.login.record</field>
	        <field name="priority">100</field>
	        <field name="arch" type="xml">
	            <tree>
	                <field name="code" />
	                <field name="name" />
	                <field name="type" />
	                <field name="online" />
	                <field name="sequence" />
	                <field name="description" />                
	            </tree>
	        </field>
	    </record>
	    <record id="view_anodoo_login_record_search" model="ir.ui.view">
	        <field name="name">anodoo.login.record.view.search</field>
	        <field name="model">anodoo.login.record</field>
	        <field name="priority">100</field>
	        <field name="arch" type="xml">
	            <search>
	            	<field name="name" filter_domain="[('name', 'ilike', self)]" />
		            <separator/>
		            <filter name="is_active_true"  string="启用" domain="[('is_active','=',True)]" /> 
		            <filter name="is_active_false"  string="未启用" domain="[('is_active','=',False)]" />               	
		           	<group expand="1" string="Group By">
		           		<filter name="groupby_active"  string="状态" context="{'group_by':'is_active'}" />               	
		           	</group>         	
	            </search>
	        </field>
	    </record>
	    
	    <!-- 动作 anodoo.login.record-->
		<record id="action_anodoo_login_record_list" model="ir.actions.act_window">
